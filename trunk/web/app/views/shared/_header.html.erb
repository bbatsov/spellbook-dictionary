<div id="wrapper">
<!-- start header -->
<div id="logo">
        <h1 class="heading"><a href="#">Spellbook</a></h1>

        <h6 class="heading"> &raquo;&nbsp;&nbsp;&nbsp;a different dictionary</h6>
</div>

<div id="header">
  <div id="menu">
    
        <ul class="main">
          <%- menu.each do |link| -%>
            <%= menu_li link %>
          <%- end -%>
        </ul>
    
  </div>
  <p class="welcome">
    <% if !current_user %>
      <%= link_to "Sign up", new_account_path %> |
      <%= link_to "Sign in", new_user_session_path %>
    <% else %>
      <%= link_to current_user.login, account_path %> |
      <%= link_to "Sign out", user_session_path, :method => :delete, :confirm => "Are you sure you want to logout?" %>
    <% end %>
  </p>
</div>
</div>